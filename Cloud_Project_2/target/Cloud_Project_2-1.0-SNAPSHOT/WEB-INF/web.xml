<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    
    <servlet>
        <servlet-name>DisplayDynamoInformationServlet</servlet-name>
        <servlet-class>DisplayDynamoInformationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DisplayDynamoInformationServlet</servlet-name>
        <url-pattern>/DisplayDynamoInformationServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ScheduleServlet</servlet-name>
        <servlet-class>com.mycompany.mediworld.ScheduleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SetReminderServlet</servlet-name>
        <servlet-class>com.mycompany.mediworld.SetReminderServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteScheduleServlet</servlet-name>
        <servlet-class>com.mycompany.mediworld.DeleteScheduleServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ScheduleServlet</servlet-name>
        <url-pattern>/ScheduleServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SetReminderServlet</servlet-name>
        <url-pattern>/SetReminderServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteScheduleServlet</servlet-name>
        <url-pattern>/DeleteScheduleServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
